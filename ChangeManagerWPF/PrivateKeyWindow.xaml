<Window x:Class="ChangeManagerWPF.Models.PrivateKeyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChangeManagerWPF.Models"
        mc:Ignorable="d"
        Title="Authentication" Height="170" Width="500">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Margin="5" Text="Enter your private key:" />
            <PasswordBox Margin="5" x:Name="privateKey" />
            <Button Margin="5" Content="OK" Click="privateKeyOK" />
            <Button Margin="5" Content="Cancel" Click="privateKeyCancel" />
        </StackPanel>
    </Grid>
</Window>
